{
  "name": "client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "lint-staged": {
    "src/**/*.{ts,tsx,json}": "eslint --fix",
    "**/*.scss": "stylelint --fix"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{ts,tsx,json}\" && stylelint \"**/*.scss\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,json}\" --fix && stylelint \"*.scss\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,scss}\"",
    "test": "vitest",
    "test:cov": "vitest run --coverage"
  },
  "dependencies": {
    "@bem-react/classname": "^1.5.12",
    "@bem-react/core": "^5.0.0",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.4",
    "bootstrap": "^5.2.3",
    "flag-icons": "^6.6.6",
    "formik": "^2.2.9",
    "i18next": "^22.4.13",
    "i18next-browser-languagedetector": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-icons": "^4.8.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "vite-tsconfig-paths": "^4.0.7",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.2",
    "@vitest/ui": "^0.29.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-stylelint": "^18.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.3.2",
    "stylelint": "^15.2.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vitest": "^0.29.2"
  }
}
